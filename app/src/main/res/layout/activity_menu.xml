<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="dane.com.gyl.dane.MenuActivity"
    android:background="#F3E624">


    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/titulo_menu"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:src="@drawable/menu_menu"
        android:layout_marginTop="10dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagen_dts"
        android:src="@drawable/menu_dts"
        android:background="#F3E624"
        android:layout_below="@+id/imagen_mnpl"
        android:layout_alignLeft="@+id/titulo_menu"
        android:layout_marginTop="50dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagen_mnpl"
        android:background="#F3E624"
        android:src="@drawable/menu_mnpl"
        android:layout_below="@+id/titulo_menu"
        android:layout_alignLeft="@+id/titulo_menu"
        android:layout_alignStart="@+id/titulo_menu"
        android:layout_marginTop="50dp" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imagen_todas"
        android:src="@drawable/menu_todas"
        android:background="#F3E624"
        android:layout_below="@+id/imagen_dts"
        android:layout_alignLeft="@+id/titulo_menu"
        android:layout_marginTop="50dp" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/moreInfo"
        android:src="@drawable/info"
        android:layout_gravity="center|right"
        android:layout_alignLeft="@+id/imagen_todas"
        android:layout_alignStart="@+id/imagen_todas"
        android:layout_marginTop="50dp"
        android:layout_below="@+id/imagen_todas" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/menu_punto_naranja"
        android:layout_above="@+id/imagen_mnpl"
        android:layout_alignRight="@+id/titulo_menu"
        android:layout_alignEnd="@+id/titulo_menu"
        android:layout_marginTop="-5dp" />


</RelativeLayout>
